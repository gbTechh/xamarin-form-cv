<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UserFormApp.DisplayPage"
             Title="Datos del Usuario">

    <ScrollView>
        <StackLayout Padding="20">
            <Label x:Name="NameLabel" />
            <Label x:Name="BirthdateLabel" />
            <Label x:Name="OccupationLabel" />
            <Label x:Name="ContactLabel" />
            <Label x:Name="NationalityLabel" />
            <Label x:Name="EnglishLevelLabel" />
            <Label x:Name="ProgrammingLanguagesLabel" />
            <Label x:Name="AptitudesLabel" />
            <Label x:Name="SkillsLabel" />
            <Label x:Name="ProfileLabel" />
        </StackLayout>
    </ScrollView>
</ContentPage>
